
					//Funciones 1. suma 

#include<stdio.h>
#include<stdlib.h>

int suma(int n1,int n2){
	
	int sumat;
	
	sumat= n1+n2;
	
	return sumat;
	
}

int main (){
	
	int num1,num2,total;
	
	printf("ingrese el numero 1° ");
	scanf("%d",&num1);
	printf("ingrese el numero 2° ");
	scanf("%d",&num2);

total = suma(num1,num2);

printf("el resultado es %d",total);	
	
	
	
	printf("\n");
	system ("pause");
	
	return 0;
}
